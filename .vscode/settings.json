{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.eslint.fixAll": true
  },
  "eslint.validate": ["javascript"],
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "cSpell.words": [
    "Cyclomatic",
    "errored",
    "noopener",
    "noreferrer",
    "stylelintrc",
    "trbl",
    "uncategorized",
    "unspaced",
    "zcong"
  ]
}
